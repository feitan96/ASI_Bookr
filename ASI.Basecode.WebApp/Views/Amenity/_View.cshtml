@model ASI.Basecode.Services.ServiceModels.AmenityViewModel

<div class="modal-header">
    <h5 class="modal-title">Amenity Details</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div>
    <dl class="row">
        @* this section is for alteast (one image of the room) *@
        <dt class="col-sm-4">
            Amenity Id
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(model => model.AmenityId)
        </dd>
        <dt class="col-sm-4">
            Amenity Name
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(model => model.AmenityName)
        </dd>
    </dl>
</div>